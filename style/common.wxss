/* Base */

body {
  background: #ebebeb;
  font-size: 16px;
  color: #333;
}

.transition {
  transition: all 0.3s ease;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
}

.main {
  width: 1200px;
  margin: 0 auto;
}

.left {
  float: left;
}

.right {
  float: right;
}

.overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.hide {
  display: none;
}

/* Layout */

.mission-list {
  padding: 12px 12px 0 12px;
}

.mission-list-main {
  background: #fff;
  margin-bottom: 12px;
  box-shadow: #1890ff 0 0 10px -3px;
}

.mission-list-main .mission-list-title {
  background: #e2f2ff;
  border-bottom: #e1e1e1 solid 1px;
  padding: 10px 12px;
}

.mission-list-main .mission-list-title image {
  width: 18px;
  margin-top: 3px;
}

.mission-list-main .mission-list-title .mission-title-name {
  width: 80%;
  margin-left: 2%;
  line-height: 150%;
}

.mission-list-main .mission-list-content {
  padding: 8px 12px;
}

.mission-list-main .mission-list-content .mission-list-content-main {
  padding: 4px 0;
}

.mission-list-main .mission-list-content image {
  width: 22px;
}

.mission-list-main .mission-list-content .mission-list-content-title {
  margin-left: 2%;
  line-height: 150%;
  vertical-align: middle;
  font-size: 14px;
  color: #666;
  width: 80px;
}

.mission-list-main .mission-list-content .mission-list-content-name {
  width: 62%;
  line-height: 150%;
  font-size: 14px;
  color: #666;
}

.work-list-main {
  background: #fff;
  margin-bottom: 12px;
  padding: 0 12px;
}

.work-list-main .work-list-title {
  border-bottom: #e1e1e1 solid 1px;
  padding: 10px 0;
}

.work-list-main .work-list-title image {
  width: 18px;
  margin-top: 3px;
}

.work-list-main .work-list-title .work-title-name {
  width: 68%;
  margin-left: 2%;
  line-height: 150%;
}

.work-list-main .work-list-title .work-title-right {
}

.work-list-main .work-list-title .work-title-right image {
  width: 28px;
  vertical-align: middle;
  margin-right: 2px;
}

.work-list-main .work-list-title .work-title-right .work-title-right-txt {
  vertical-align: middle;
  display: inline-block;
  font-size: 14px;
}

.work-list-main .work-list-content {
  padding: 8px 0;
}

.work-list-main .work-list-content .work-list-content-main {
  padding: 4px 0;
  font-size: 14px;
  color: #666;
}

.work-list-main .work-list-content image {
  width: 22px;
}

.work-list-main .work-list-content .work-list-content-title {
  margin-left: 2%;
  line-height: 150%;
  vertical-align: middle;
  width: 80px;
}

.work-list-main .work-list-content .work-list-content-name {
  width: 62%;
  line-height: 150%;
}

.work-list-main .work-list-content .red-txt {
  color: #e20105;
}

.work-detail-main {
  background: #fff;
  margin-bottom: 12px;
  padding: 0 12px;
}

.work-detail-main .work-detail-title {
  padding: 10px 0;
}

.work-detail-main .work-detail-title image {
  width: 22px;
  vertical-align: middle;
  margin-right: 6px;
}

.work-detail-main .work-detail-title > view {
  vertical-align: middle;
  font-weight: bold;
  display: inline-block;
}

.work-detail-main .detail-main {
  border-bottom: #e1e1e1 solid 1px;
  padding: 12px 0;
  line-height: 150%;
}

.work-detail-main .detail-main .detail-main-title {
  width: 30%;
}

.work-detail-main .detail-main .detail-main-content {
  width: 65%;
}

.work-detail-main .detail-main .detail-main-txt {
  display: inline-block;
  word-break: break-all;
}

.work-detail-main .detail-main image {
  width: 20%;
}

.work-detail-main .detail-main .detail-main-adr {
  width: 80%;
}

.work-detail-main .check-main {
  border-bottom: #e1e1e1 solid 4px;
}

.big-btn {
  padding: 16px;
}

.big-btn .big-btn-main {
  background: #1890ff;
  padding: 8px 0;
  text-align: center;
  color: #fff;
}

.two-btn .big-btn-main {
  width: 50%;
}

.two-btn .other-btn-main {
  background: #8bc7ff;
}

.big-btn .big-btn-main image {
  vertical-align: middle;
  margin-right: 6px;
  width: 34px;
}

.big-btn .big-btn-main > view {
  vertical-align: middle;
  display: inline-block;
}

.entering-box {
  background: #fff;
  margin-bottom: 12px;
}

.entering-box .entering-box-title {
  border-bottom: #e1e1e1 solid 1px;
  padding: 10px 12px;
}

.entering-box .entering-box-title image {
  width: 24px;
  vertical-align: middle;
  margin-right: 6px;
}

.entering-box .entering-box-title > view {
  vertical-align: middle;
  font-weight: bold;
  display: inline-block;
}

.red-star {
  color: #e20105;
  display: inline-block;
  margin-left: 4px;
}

.entering-content {
  padding: 0 12px 12px 12px;
}

.entering-content .entering-main {
  background: #e2f2ff;
  margin-top: 12px;
  padding: 8px 12px;
}

.entering-main .entering-content-main {
  padding: 6px 0;
}

.entering-main .entering-content-title {
  line-height: 150%;
  vertical-align: middle;
  width: 100px;
}

.entering-main .entering-law-title {
  line-height: 30px;
}

.entering-main .entering-content-name {
  width: 62%;
  line-height: 150%;
}

.entering-main .input-select {
  border: #e1e1e1 solid 1px;
  width: 100%;
  height: 30px;
  font-size: 16px;
}

.entering-main .input-select-long {
  border: #e1e1e1 solid 1px;
  width: 100%;
  height: 30px;
  font-size: 16px;
  margin-top: 12px;
}

.entering-main .input-txt {
  border: #e1e1e1 solid 1px;
  width: 100%;
  height: 30px;
  padding: 0 6px;
  box-sizing: border-box;
  font-size: 16px;
}

.entering-box .input-txt-long {
  border: #e1e1e1 solid 1px;
  width: 100%;
  height: 30px;
  padding: 0 6px;
  box-sizing: border-box;
  font-size: 16px;
  margin-top: 12px;
}

.entering-box .input-select-long {
  border: #e1e1e1 solid 1px;
  width: 100%;
  height: 30px;
  font-size: 16px;
  margin-top: 12px;
}

.entering-box .input-textarea-long {
  border: #e1e1e1 solid 1px;
  width: 100%;
  height: 150px;
  font-size: 16px;
  margin-top: 12px;
  resize: none;
}

.upload-box {
  margin-top: 12px;
}

.upload-box .upload-btn {
  border: #e1e1e1 dashed 2px;
  width: 90px;
  height: 90px;
  box-sizing: border-box;
  margin-right: 12px;
}

.upload-box .upload-btn image {
  width: 100%;
}

.upload-box .upload-pic {
  width: 90px;
  height: 90px;
  box-sizing: border-box;
  margin-right: 12px;
  position: relative;
}

.upload-box .upload-pic image {
  width: 100%;
}

.upload-box .upload-pic .close-btn {
  background: #fe0000 url(data:image/png;base64,) no-repeat center center;
  width: 26px;
  height: 26px;
  border-radius: 100px;
  position: absolute;
  top: -10px;
  right: -10px;
}

.login {
  background: -webkit-linear-gradient(#1890ff, #0f77d6);
  background: -o-linear-gradient(#1890ff, #0f77d6);
  background: -moz-linear-gradient(#1890ff, #0f77d6);
  background: linear-gradient(#1890ff, #0f77d6);
  background-repeat: no-repeat;
  background-size: 100% 66%;
  padding: 40px 12px;
}

.login image {
  display: block;
  width: 80%;
  margin: 0 auto;
}

.login-box {
  background: #fff;
  box-shadow: #1890ff 0 0 10px -3px;
  border-radius: 4px 4px 0 0;
}

.login-box .login-main-box {
  padding: 12px;
}

.login-box .login-main-box .login-main-content {
  padding: 6px 0;
}

.login-box .login-main-box .login-main-content .input-txt {
  background-color: #f5f6f8;
  background-repeat: no-repeat;
  background-position: 6px center;
  background-size: 34px;
  border: none;
  /* width: 100%; */
  height: 40px;
  padding: 0 6px 0 46px;
  font-size: 16px;
  border-radius: 4px;
}

.login-box .login-main-box .login-main-content .input-user {
  background-image: url(data:image/png;base64,);
}

.login-box .login-main-box .login-main-content .input-pwd {
  background-image: url(data:image/png;base64,);
}

.login-box .login-btn {
  background: #1890ff;
  padding: 12px 0;
  text-align: center;
  color: #fff;
  border-radius: 0 0 4px 4px;
  font-weight: bold;
}

.my-info {
  background: url(data:image/jpeg;base64,) no-repeat center top;
  background-size: 100%;
  padding: 12px;
  color: #fff;
}

.my-info .my-info-left {
  width: 70%;
}

.my-info .my-info-left .my-name {
  font-size: 18px;
  font-weight: bold;
  margin-bottom: 6px;
}

.my-info .my-info-left .my-dept {
  font-size: 14px;
  color: #d2e3ff;
}

.my-info .my-info-right {
  width: 30%;
  text-align: right;
}

.my-info .my-info-right image {
  width: 28px;
  vertical-align: middle;
  margin-right: 4px;
}

.my-info .my-info-right .my-info-right-txt {
  vertical-align: middle;
  display: inline-block;
  font-size: 14px;
}
